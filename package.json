{
  "name": "inputs",
  "version": "1.0.0",
  "description": "",
  "main": "./lib",
  "module": "./src/components",
  "scripts": {
    "build": "babel ./src --out-dir ./lib",
    "dev": "webpack-dev-server --inline --content-base ./ --env.dev",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "watch:test": "mocha --watch --compilers js:babel-register",
    "start": "http-server",
    "setup": "npm install && npm run validate",
    "validate": "npm-run-all lint test build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ModulUI/inputs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ModulUI/inputs/issues"
  },
  "homepage": "https://github.com/ModulUI/inputs#readme",
  "dependencies": {
    "accounting": "^0.4.1",
    "classnames": "^2.1.2",
    "prop-types": "^15.5.8",
    "modul-helpers": "0.1.8",
    "jquery-datetimepicker": "^2.5.4",
    "reselect": "^3.0.0",
    "tether-drop": "^1.4.2",
    "react-modal": "^1.9.3",
    "react-notification-system": "^0.2.14",
    "react-tooltip": "git://github.com/benderr/react-tooltip.git#v3.3.5",
    "q": "^1.4.1"
  },
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0",
    "react-dom": "^0.14.9 || ^15.3.0 || ^16.0.0-rc || ^16.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-router": "2.6.1",
    "history": "^3.0.0",
    "bootstrap": "^3.3.5",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-plugin-istanbul": "1.0.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "6.11.6",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.0",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.2.2",
    "eslint-config-kentcdodds": "^9.0.2",
    "file-loader": "0.9.0",
    "http-server": "0.9.0",
    "jsdom": "9.4.1",
    "lodash": "4.14.2",
    "mocha": "3.0.2",
    "npm-run-all": "2.3.0",
    "nyc": "7.1.0",
    "nib": "^1.1.2",
    "react-addons-test-utils": "15.3.0",
    "react-select": "^1.0.0-rc.10",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "starwars-names": "1.6.0",
